CREATE TABLE  Hiragana (
 id INT NOT NULL AUTO_INCREMENT ,
 consonant VARCHAR( 1 ) ,
 vowel VARCHAR( 32 ) ,
 consonantOrder Int NOT NULL ,
 vowelOrder Int NOT NULL ,
 romaji VARCHAR( 8 ) ,
 pronunciation VARCHAR( 8 )
PRIMARY KEY (  id )
)CHARACTER SET utf8 COLLATE utf8_general_ci;
ALTER TABLE Hiragana ADD INDEX(consonant);
ALTER TABLE Hiragana ADD INDEX(vowel);
ALTER TABLE Hiragana ADD INDEX(consonantOrder);
ALTER TABLE Hiragana ADD INDEX(vowelOrder);
